hist: actual_int_per_crossing
    var:    actual_int_per_crossing
    x_bins: 50
    x_min:  0
    x_max:  50.
    title:  actual interactions per crossing
    x_axis: actual interactions per crossing
    y_axis: Entries
end

hist: average_int_per_crossing
    var:    average_int_per_crossing
    x_bins: 50
    x_min:  0
    x_max:  50.
    title:  average interactions per crossing
    x_axis: average interactions per crossing
    y_axis: Entries
end

hist: mll
    var:    mll/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  m_{ll}
    x_axis: m_{ll} [GeV]
    y_axis: Entries/10 GeV
end

hist: mt2
    var:    mt2/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  m_{T2}
    x_axis: m_{T2} [GeV]
    y_axis: Entries/10 GeV
end

hist: ptll
    var:    ptll/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  p_{T}^{ll}
    x_axis: p_{T}^{ll} [GeV]
    y_axis: Entries/10 GeV
end

hist: met
    var:    met_et/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  E_{T}^{miss}
    x_axis: E_{T}^{miss} [GeV]
    y_axis: Entries/10 GeV
end

hist: met_rel
    var:    met_rel_et/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  E_{T}^{miss, rel}
    x_axis: E_{T}^{miss, rel} [GeV]
    y_axis: Entries/10 GeV
end

hist: mu_0_mt
    var:    sqrt( 2*mu_pt[0]*met_et*(1-cos(met_phi - mu_phi[0]) ) )/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  m_{T}^{\mu, leading}
    x_axis: m_{T}^{\mu, leading} [GeV]
    y_axis: Entries/10 GeV
end

hist: mu_1_mt
    var:    sqrt( 2*mu_pt[1]*met_et*(1-cos(met_phi - mu_phi[1]) ) )/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  m_{T}^{\mu, sub-leading}
    x_axis: m_{T}^{\mu, sub-leading} [GeV]
    y_axis: Entries/10 GeV
end
