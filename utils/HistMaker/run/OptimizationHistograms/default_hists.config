hist: mll
    var:    mll/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  m_{ll}
    x_axis: m_{ll} [GeV]
    y_axis: Entries/10 GeV
end

hist: met
    var:    met_et/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  E_{T}^{miss}
    x_axis: E_{T}^{miss} [GeV]
    y_axis: Entries/10 GeV
end

hist: met_rel
    var:    met_rel_et/1000.
    x_bins: 50
    x_min:  0
    x_max:  500.
    title:  E_{T}^{miss, rel}
    x_axis: E_{T}^{miss, rel} [GeV]
    y_axis: Entries/10 GeV
end

hist: met_rel_delta_phi
    var:    met_rel_delta_phi
    x_bins: 64
    x_min:  -6.4
    x_max:  +6.4.
    title:  \delta\phi(E_{T}^{miss,rel}, object)
    x_axis: \delta\phi(E_{T}^{miss,rel}, object)
    y_axis: Entries
end
