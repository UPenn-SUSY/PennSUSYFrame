# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
cut: ee_z_window
    pass_grl:               1
    pass_incomplete_event:  1
    pass_lar_error:         1
    pass_tile_error:        1
    pass_tile_hot_spot:     1
    pass_tile_trip:         1
    pass_bad_jets:          1
    pass_calo_problem_jets: 1
    pass_primary_vertex:    1
    pass_bad_muons:         1
    pass_cosmic_muons:      1
    pass_hfor:              1
    pass_ge_2_good_leptons: 1
    pass_2_good_leptons:    1
    pass_mll:               1
    pass_trigger_match:     1
    pass_flavor:            ee
    pass_ee_trigger:        1
    pass_mm_trigger:        0
    pass_em_trigger:        0
    pass_me_trigger:        0

    pass_sr_ssjets:  0

    pass_l_jet_veto: 0
    pass_b_jet_veto: 1
    pass_f_jet_veto: 1
    pass_z_veto:     0
    pass_top_veto:   1

    pass_sign_channel: ss

    additional_cut: el_pt[0] > 10e3
    additional_cut: el_pt[1] > 10e3

    additional_cut: light_jet_num_light_jets >= 0

    additional_cut: met_rel_et > 20e3

    additional_cut: mll > 81e3
    additional_cut: mll < 101e3
end

